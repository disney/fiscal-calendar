{
  "name": "disney-fiscal-calendar",
  "version": "1.0.2",
  "description": "A library for working with Disney fiscal calendar dates",
  "author": "Geoffrey Booth <geoffrey.booth@disney.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.disney.com/Geoffrey.Booth/disney-fiscal-calendar.git"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "coffee --bare --no-header --output ./dist ./src",
    "test": "ava",
    "dev": "coffee --bare --no-header --output ./dist --watch ./src & ava --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@date/holidays-us": "^0.4.0",
    "luxon": "^1.12.1"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "coffeescript": "^2.4.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "coffee"
    ],
    "require": [
      "coffeescript/register"
    ],
    "files": [
      "tests/**/*.coffee"
    ]
  }
}
